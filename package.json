{
  "name": "holistic-router",
  "description": "versatile server and client router for small single page sites",
  "version": "0.2.8",
  "homepage": "https://github.com/paulpflug/",
  "author": {
    "name": "Paul Pflugradt",
    "email": "paul.pflugradt@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/paulpflug/holistic-router"
  },
  "engines": {
    "node": "*"
  },
  "main": "server.js",
  "browser": "client.js",
  "files": [
    "*.js"
  ],
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "chokidar": "^1.7.0",
    "consolidate": "^0.14.5",
    "fs-extra": "^4.0.0"
  },
  "devDependencies": {
    "autodev": "^0.1.1",
    "coffee-cli": "^0.2.1",
    "coffee-loader": "^0.7.3",
    "coffee-script": "^1.12.7",
    "get-locale": "^0.0.1",
    "koa": "^2.3.0",
    "koa-hot-dev-webpack": "^0.1.11",
    "koa-static": "^4.0.1",
    "marked": "^0.3.6",
    "pug": "^2.0.0-rc.2",
    "script-runner": "^0.1.7",
    "webpack": "^3.3.0"
  },
  "keywords": [
    "koa",
    "router",
    "html5"
  ],
  "readmeFilename": "README.md",
  "scripts": {
    "build": "run-npm build:*",
    "build:server": "coffee-cli --next --bare --no-header --compile --output . src/*.coffee",
    "build:client": "coffee-cli --no-header --compile --output . src/client.coffee",
    "dev": "autodev ./dev/server.coffee",
    "dev:locale": "autodev ./dev/serverLocale.coffee",
    "test": "",
    "preversion": "npm run test",
    "version": "npm run build && git add .",
    "postversion": "git push && git push --tags && npm publish"
  }
}
